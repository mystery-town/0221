    },
    {
        "type": "message",
        "user": "U3U0TU7K9",
        "text": "Well this all works.",
        "ts": "1484769203.000045"
    },
    {
        "type": "message",
        "user": "U3SJ6J3CZ",
        "text": "I do much prefer this. ",
